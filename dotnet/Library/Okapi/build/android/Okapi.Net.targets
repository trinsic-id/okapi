<?xml version="1.0" encoding="utf-8"?>
<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">

  <ItemGroup Condition="'$(TargetFrameworkIdentifier)' == 'net5.0-android'">
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)..\..\native\android\arm64-v8a\libokapi.so">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Abi>arm64-v8a</Abi>
    </AndroidNativeLibrary>
    
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)..\..\native\android\armeabi-v7a\libokapi.so">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Abi>armeabi-v7a</Abi>
    </AndroidNativeLibrary>
    
    <AndroidNativeLibrary Include="$(MSBuildThisFileDirectory)..\..\native\android\x86\libokapi.so">
      <CopyToOutputDirectory>Always</CopyToOutputDirectory>
      <Abi>x86</Abi>
    </AndroidNativeLibrary>
  </ItemGroup>

</Project>
